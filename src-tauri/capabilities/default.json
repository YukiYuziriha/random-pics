{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "remote": {
    "urls": [
      "http://127.0.0.1:3000/*",
      "http://localhost:3000/*"
    ]
  },
  "permissions": [
    "core:default",
    "dialog:allow-open"
  ]
}
